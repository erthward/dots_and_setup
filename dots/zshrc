if [ -f '/home/deth/Desktop/tmp_google_cloud_SDK/google-cloud-sdk/completion.zsh.inc' ]; then . '/home/deth/Desktop/tmp_google_cloud_SDK/google-cloud-sdk/completion.zsh.inc'; fi

